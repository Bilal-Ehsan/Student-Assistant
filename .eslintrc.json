{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": 0,
    "comma-dangle": 0,
    "arrow-parens": 0,
    "no-console": 0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "no-shadow": 0
  }
}
